{
  "name": "hotel",
  "version": "0.8.7",
  "description": "Local domains for everyone and more! ",
  "main": "lib",
  "bin": "lib/cli/bin.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "ava && npm run lint",
    "lint": "eslint . --ignore-path .gitignore && stylelint './src/app/**/*.css'",
    "fix": "eslint . --ignore-path .gitignore --fix && stylelint './src/app/**/*.css' --fix",
    "start": "run-p start:*",
    "start:webpack": "webpack-dev-server --config webpack.dev.js --open",
    "start:nodemon": "nodemon -- src/daemon",
    "prepublishOnly": "npm run build && pkg-ok",
    "uninstall": "node bin/uninstall.js",
    "build": "run-s build:*",
    "build:webpack": "rimraf dist && webpack --config ./webpack.prod.js",
    "build:babel": "rimraf lib && babel src -d lib --copy-files --ignore src/app",
    "addFixtures": "node src/cli/bin add \"node index\" --dir ./test/fixtures/app && node src/cli/bin add \"node index\" --dir ./test/fixtures/verbose && node src/cli/bin add http://some-domain.com --name local-domain "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skkim7821/hotel.git"
  },
  "keywords": [
    "dev",
    "devtool",
    "domain",
    "host",
    "https",
    "local",
    "localhost",
    "manager",
    "process",
    "proxy",
    "server"
  ],
  "author": "Typicode <typicode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skkim7821/hotel/issues"
  },
  "homepage": "https://github.com/skkim7821/hotel",
  "dependencies": {
    "after-all": "^2.0.2",
    "ansi2html": "0.0.1",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "connect-sse": "^1.2.0",
    "exit-hook": "^2.2.1",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "http-proxy": "^1.18.1",
    "matcher": "^4.0.0",
    "mkdirp": "^1.0.4",
    "once": "^1.4.0",
    "please-upgrade-node": "^3.2.0",
    "pug": "^3.0.2",
    "respawn": "^2.6.0",
    "run-node": "^2.0.0",
    "selfsigned": "^1.10.8",
    "server-ready": "^0.3.1",
    "sudo-block": "^3.0.0",
    "tildify": "^2.0.0",
    "tinydate": "^1.3.0",
    "unquote": "^1.1.1",
    "untildify": "^4.0.0",
    "update-notifier": "^5.1.0",
    "user-startup": "^0.2.2",
    "vhost": "^3.0.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/escape-html": "1.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-icons": "^2.2.7",
    "ava": "^3.15.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "classnames": "^2.2.6",
    "css-loader": "^5.1.3",
    "escape-html": "^1.0.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.1.3",
    "lodash.uniqueid": "^4.0.1",
    "mobx": "^6.1.8",
    "mobx-react": "^7.1.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "pkg-ok": "^2.3.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^21.0.0",
    "supertest": "^6.1.3",
    "tempy": "^1.0.1",
    "ts-loader": "^8.0.18",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.2.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.26.3",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "ava": {
    "serial": true,
    "verbose": true,
    "require": [
      "babel-register",
      "./test/_setup"
    ],
    "babel": "inherit"
  }
}
